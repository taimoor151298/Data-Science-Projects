      "source": [
        "# Create a marker with distance to a closest city, railway, railway, etc.\n",
        "# Draw a line between the marker to the launch site\n",
        "# find coordinate of the closet coastline\n",
        "#e.g.,: Lat: 28.56367  Lon: -80.57163\n",
        "\n",
        "launch_site_lat = 28.563197\n",
        "launch_site_lon = -80.576820\n",
        "railway_lat = 28.57201\n",
        "railway_lon = -80.58521\n",
        "distance_railway = calculate_distance(launch_site_lat, launch_site_lon, railway_lat, railway_lon)\n",
        "distance_railway\n",
        "\n",
        "# Create and add a folium.Marker on your selected closest railway point on the map\n",
        "# Display the distance between highway point and launch site using the icon property \n",
        "# for example\n",
        "coordinate = [highway_lat, highway_lon]\n",
        "distance_marker = folium.Marker(coordinate,\n",
        "   icon=DivIcon(\n",
        "        icon_size=(20,20),\n",
        "        icon_anchor=(0,0),\n",
        "        html='<div style=\"font-size: 12; color:#d35400;\"><b>%s</b></div>' % \"{:10.2f} KM\".format(distance_highway),\n",
        "        )\n",
        "    )\n",
        "site_map.add_child(distance_marker)\n",
        "\n",
        "# Create a `folium.PolyLine` object using the highway coordinates and launch site coordinate\n",
        "coordinates = [[launch_site_lat, launch_site_lon], [highway_lat, highway_lon]]\n",
        "lines=folium.PolyLine(locations=coordinates, weight=1)\n",
        "site_map.add_child(lines)"
      ]
